","generated":{"line":90,"column":2},"source":"components/course/viewCourse/viewCourse.jsx","original":{"line":4,"column":25}},{"name":"ViewCourse","generated":{"line":92,"column":15},"source":"components/course/viewCourse/viewCourse.jsx","original":{"line":65,"column":15}}],"sources":{"components/course/viewCourse/viewCourse.jsx":"import React, {Component} from 'react';\r\nimport Axios from 'axios';\r\n\r\nclass ViewCourse extends Component {\r\n    constructor(){\r\n        super();\r\n        this.state = {\r\n            course: [],\r\n            subject: []\r\n        }\r\n    }\r\n\r\n    componentDidMount() {\r\n        Axios.get('/api/course').then(\r\n            data => {\r\n                this.setState({\r\n                    course: data.data\r\n                })\r\n            }\r\n        )\r\n    }\r\n\r\n    subject(id){\r\n        Axios.get('/api/subject/find/' + id).then(\r\n            dataSet => {\r\n                alert('Name: ' + dataSet.data.name + 'Description: ' + dataSet.data.description);\r\n            }\r\n        )\r\n    }\r\n\r\n    render() {\r\n        return(\r\n          <div className=\"container\">\r\n              <h2>This is Cousre View</h2>\r\n              <table className=\"table\">\r\n                  <thead>\r\n                    <tr>\r\n                      <th>Name</th>\r\n                      <th>Code</th>\r\n                      <th>Passs Mark</th>\r\n                      <th>Lecturer Incharge</th>\r\n                      <th>Subjects</th>\r\n                    </tr>\r\n                      </thead>\r\n                  <tbody>\r\n                  {\r\n                      this.state.course.map(cou => {\r\n                          return(\r\n                              <tr key={cou._id}>\r\n                                  <td>{cou.name}</td>\r\n                                  <td>{cou.code}</td>\r\n                                  <td>{cou.passMark}</td>\r\n                                  <td>{cou.lecIncharge}</td>\r\n                                  <td><button onClick={this.subject.bind(this,cou.subject)}>Subjects</button></td>\r\n                              </tr>\r\n                          )\r\n                      })\r\n                  }\r\n                  </tbody>\r\n              </table>\r\n          </div>\r\n        );\r\n    }\r\n}\r\nexport default ViewCourse\r\n"},"lineCount":null}},"error":null,"hash":"23bb4b8e32e916090a602caf82e1e7e1","cacheData":{"env":{}}}